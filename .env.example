# Токен Telegram-бота
TELEGRAM_TOKEN=your_bot_token_here

# Чат ID или @username канала/чата
CHAT_ID=your_chat_id_here

# RSS-лента для парсинга
RSS_URL=https://www.wired.com/feed/rss

# Кол-во новостей за один раз
NEWS_LIMIT=5

# Интервал между проверками в секундах (10 минут = 600)
INTERVAL=600

# Имя файла для хранения отправленных ссылок
SENT_LINKS_FILE=sent_links.json
